  "version": "0.2.0",
  "configurations": [
    {
      "name": "Add tasks to Alice",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "MODE": "insert",
        "LOOKUP_KEY": "alice"
      },
      "program": "${fileDirname}"
    },
    {
      "name": "Add tasks to Bob",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "MODE": "insert",
        "LOOKUP_KEY": "bob"
      },
      "program": "${fileDirname}"
    },
    {
      "name": "Run workers",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "MODE": "worker",
        "LOOKUP_KEY": "bob"
      },
      "program": "${fileDirname}"
    }
  ]
}
